{
  "build": {
    "command": "npm run build",
    "directory": "out",
    "root": "/",
    "environment": {
      "NODE_VERSION": "18"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/_next/static/(.*)",
      "headers": {
        "Cache-Control": "public,max-age=31536000,immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 